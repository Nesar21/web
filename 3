<!-- ✅ File: frameset_layout.html -->
<!DOCTYPE html>
<html>
<head>
  <title>2x2 Frame Layout</title>
</head>
<frameset rows="50%,50%">
  <!-- Top Row -->
  <frameset cols="50%,50%">
    <frame src="top-left.html" name="topLeft">
    <frame src="top-right.html" name="topRight">
  </frameset>

  <!-- Bottom Row -->
  <frameset cols="50%,50%">
    <frame src="bottom-left.html" name="bottomLeft">
    <frame src="bottom-right.html" name="bottomRight">
  </frameset>
</frameset>
</html>

